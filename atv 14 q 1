#include <stdio.h>
#include <stdlib.h>

int main(void) {

  int *array = malloc(5);
  printf("Digite 5 n√∫meros\n");
  for (int i = 0; i < 5; i++){
    scanf("%d", &array[i]);
  }
  printf("Array Digitado: ");
  for (int i = 0; i < 5; i++){
    printf("%d, ", array[i]);
  }
  
  free(array);
  array = NULL;
  
  return 0;
}
